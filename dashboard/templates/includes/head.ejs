<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<% function capitalize(string) {
 return string.charAt(0).toUpperCase() + string.slice(1);
} %>
<title><% if(title) { %><%- capitalize(bot.user.username) + ' | ' + title %><% } else { %><%- capitalize(bot.user.username) + ' - The best Discord bot' %><% } %></title>

<link rel="stylesheet" type="text/css" href="<%= secure_connection %><%= domain %>/css/style.css" media="all">
<%- include('./favicons.ejs') %>
<% if (config.google_analitics) { %>
<%- include('./analytics.ejs') %>
<% } %>
<%- include("./seo", { bot, user, path, description, title, twitter, url }) %>
<% if(config.arc_token) { %>
<script async src="https://arc.io/widget.min.js#<%= config.arc_token %>"></script>
<% } %>
</head>
<body>
<script type="text/javascript">var preload = document.createElement('div'); preload.className = "preloader"; preload.innerHTML = '</div><div class="spinner"></div>'; document.body.appendChild(preload); window.addEventListener('load', function() { preload.className +=  ' fade'; setTimeout(function(){ preload.style.display = 'none'; },500); })</script>
<noscript><div class="no-js">To view the page please enable javascript in your browser.</div></noscript>
<%- include("./nav", { bot, user, path, url, name, tag }) %>
<div class="blur" id="body-blur"></div>
<%- title === "Dashboard" ? "" : "" %>
